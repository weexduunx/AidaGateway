{
    "name": "weexduunx/laravel-aida-gateway",
    "description": "Package Laravel pour int√©grer facilement les paiements Mobile Money (Orange Money, Wave, Free Money, E-money)",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "mobile-money",
        "payment",
        "orange-money",
        "wave",
        "free-money",
        "e-money",
        "aida",
        "gateway"
    ],
    "authors": [
        {
            "name": "Idrissa Ndiouck",
            "email": "indiouck04@yahoo.fr"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Weexduunx\\AidaGateway\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Weexduunx\\AidaGateway\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Weexduunx\\AidaGateway\\AidaGatewayServiceProvider"
            ],
            "aliases": {
                "Aida": "Weexduunx\\AidaGateway\\Facades\\Aida"
            }
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "test-coverage-text": "phpunit --coverage-text",
        "cs-check": "php-cs-fixer fix --dry-run --diff",
        "cs-fix": "php-cs-fixer fix"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
